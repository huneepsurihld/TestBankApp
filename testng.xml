<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Bank APP">


<listeners>
<listener class-name="Util.ExtentList"></listener>
<listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
</listeners>

 <parameter name="author" value="Huneep"/>

<!--Test to run on Chrome-->

  <test name="Bank Manager -- chrome">
  
  <parameter name="browser" value="chrome"/>
  <parameter name="name" value="CHROME"/>
 
   
    <classes>
            
      <class name="TestCases.SampleFormTest"/>
      <class name="TestCases.HomePageTest"/>
      <class name="TestCases.TestLifeTimeMembership"/>
      <class name="TestCases.BankManagerTest"/>
      <class name="TestCases.CustLoginTest"/>
      
    </classes>
  </test>
  
   <!-- Test to run on Firefox-->
  
  <test name="Bank Manager -- firefox">
  
  <parameter name="browser" value="firefox"/>
  <parameter name="name" value="FIREFOX"/>
  
  
   <classes>
    
            
     <class name="TestCases.SampleFormTest"/> 
 	 <class name="TestCases.TestLifeTimeMembership"/>
  	 <class name="TestCases.HomePageTest"/>
     <class name="TestCases.BankManagerTest"/>
     <class name="TestCases.CustLoginTest"/>
       
    </classes>
  </test>
  
</suite> <!-- Suite -->
